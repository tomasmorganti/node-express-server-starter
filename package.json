{
    "name": "node-express-server-starter",
    "version": "0.0.0",
    "description": "",
    "scripts": {
        "dev-build": "docker-compose build",
        "dev-start": "docker-compose -p dev up -d && docker logs server -f",
        "dev-stop": "docker-compose -p dev down",
        "dev-add-package": "docker-compose -p dev exec -w /usr/local node npm install --save",
        "nodemon": "nodemon",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write",
        "prepare": "husky install",
        "test": "jest --forceExit --detectOpenHandles",
        "test-integration": "sh ./run-integration-tests.sh",
        "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
        "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
        "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
        "make:seed": "knex seed:make --knexfile=knexfile.ts -x ts",
        "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts",
        "build": "tsc && tsc-alias",
        "start": "node dist/server.js"
    },
    "keywords": [],
    "author": "Tomas Morganti",
    "license": "MIT",
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.9.2",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.2",
        "jest": "^27.2.0",
        "jest-extended": "^0.11.5",
        "nodemon": "^2.0.12",
        "prettier": "^2.4.1",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "tsc-alias": "^1.3.9",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "ajv": "^8.6.3",
        "ajv-formats": "^2.1.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "envalid": "^7.2.1",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^5.3.0",
        "express-winston": "^4.2.0",
        "helmet": "^4.6.0",
        "knex": "^0.21.21",
        "objection": "^2.2.16",
        "pg": "^8.7.1",
        "uid": "^2.0.0",
        "uuid": "^8.3.2"
    }
}
